-* File ars_cdg_fil_statut_plafond.fex
TABLE FILE SI_AXN_AXE_NIVEAU
PRINT *
WHERE ID_AXE EQ 'AARS15EFF'
WHERE ID_NIVEAU EQ '2'
ON TABLE HOLD AS FIL_STATUT_PLAFOND FORMAT ALPHA
END
 
JOIN CLEAR *
-RUN
JOIN
 ID_VALEUR_AXE IN FIL_STATUT_PLAFOND TO UNIQUE ID_VALEUR_AXE
 IN SI_NIV_NIVEAU AS J4
END
 
TABLE FILE FIL_STATUT_PLAFOND
PRINT ID_VALEUR_AXE
	  LIBELLE_NIV
BY ORDRE_LISTE NOPRINT
ON TABLE PCHOLD FORMAT XML
END
 
 
 
